{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "jsx-no-lambda": false,
    "jsx-a11y/anchor-is-valid": 0,
    "no-console": false
  },
  "jsRules": {
    "no-console": false
  }
}